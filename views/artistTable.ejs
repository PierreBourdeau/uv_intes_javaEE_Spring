<% for(let i =0; i<artists.length; i++) {%>
<table class="table table-hover">
    <thead>
        <%= artists[i].artist %>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Title</th>
        <th scope="col">Length</th>
        <th scope="col">Genre</th>
      </tr>
    </thead>
    <tbody>
        <% for(let j=0; j<artists[i].songs.length; j++) {%>
            <tr>
              <th scope="row"><%= j+1 %></th>
              <td><%= artists[i].songs[j].title%></td>
              <td>@<%= artists[i].songs[j].length%></td>
              <td><%= artists[i].songs[j].genre%></td>
            </tr>
            <% } %>
    </tbody>
  </table>
  <%}%>