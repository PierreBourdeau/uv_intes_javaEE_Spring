<% if(music != 'empty') { %>
<table class="table table-hover">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Title</th>
        <th scope="col">Artist</th>
        <th scope="col">Length</th>
        <th scope="col">Genre</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <th scope="row"><%= 1 %></th>
        <td><%= music.title%></td>
        <td><%= music.artist%></td>
        <td>@<%= music.length%></td>
        <td><%= music.genre%></td>
      </tr>
    </tbody>
  </table>
<%}%>
<% if(music == 'empty') { %>
    <div class="alert alert-danger" role="alert">
        No result found !
    </div>
<%}%>